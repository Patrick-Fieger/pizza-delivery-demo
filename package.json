{
  "name": "pizza-delivery-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-vue": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "postinstall": "npm run build",
    "start": "node web-server.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend-dev": "nodemon bin/www"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.17.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "core-js": "2.6.9",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "helmet": "^3.23.3",
    "highlight.js": "^10.7.3",
    "jwks-rsa": "^1.12.3",
    "morgan": "^1.10.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.14"
  },
  "prettier": {
    "singleQuote": false,
    "semi": true
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "plugins": [
      "vue"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "error",
            "warn"
          ]
        }
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
